{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "@vue/typescript"
  ],
  "rules": {
    "comma-dangle": [
      "error",
      "always-multiline"
    ]
  },
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [".ts"]
    }
  },
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "project": [
      "./tsconfig.json"
    ],
    "ecmaVersion": 2018,
    "sourceType": "module"
  }
}
